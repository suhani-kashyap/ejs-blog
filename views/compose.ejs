<%- include("partials/header") -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input class="form-control" type="text" name="newPostTitle">
        </div>
        
        <div class="form-group">
            <label>Post</label>
            <textarea class="form-control" type="text" name="newPost" row="10" cols="100"></textarea>
        </div>
        
        <button class="btn btn-primary" type="submit">Publish</button>
    </form>
    
<%- include("partials/footer") -%>